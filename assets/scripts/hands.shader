models/weapons2/handskins/hand_white
{
	{
		map models/weapons2/handskins/hand_swat.jpg
		rgbGen lightingDiffuse
	}
	{
		map models/weapons2/handskins/hand_white.tga
		blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
		alphaFunc GT0
		depthWrite
		rgbGen lightingDiffuse
	}
}


