models/players/orion/lower_white
{
	{
		map models/players/orion/lower_swat_w.tga
		rgbGen lightingDiffuse
	}
	{
		map models/players/orion/colors/lower_white.tga
		blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
		alphaFunc GT0
		depthWrite
		rgbGen lightingDiffuse
	}


}


